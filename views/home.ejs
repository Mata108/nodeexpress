<%- include('./header.ejs' ) %>

 


  <div class=" mt-4">
    
    <div class="carousel slide" data-bs-ride="carousel" id="aa">

      <div class="carousel-indicators">
        <button type="button" data-bs-target="#aa" data-bs-slide-to="0" class="active"></button>
        <button type="button" data-bs-target="#aa" data-bs-slide-to="1"></button>
        <button type="button" data-bs-target="#aa" data-bs-slide-to="2"></button>
      </div>
      <div class="carousel-inner">
        <div class="carousel-item active" data-bs-interval="4000">

          <img
            src="https://web.mitsgwalior.in/images/Photo-Gallery/8.jpeg"
              class="w-100" height="650x" width="893" alt="...">
          <div class="carousel-caption">
            <h1> Glory of Mits  </h1>
          </div><!--carousel-caption-->
        </div>
        <div class="carousel-item" data-bs-interval="2000">

          <img src="https://images.collegedunia.com/images/profile/2023-04-26/1682525218_MITSgwalior.jpeg"
            class="w-100" alt="...">
          <div class="carousel-caption">
            <h1> Mits collage </h1>
          </div><!--carousel-caption-->
        </div>
        <div class="carousel-item" data-bs-interval="2000">

          <img
            src="https://web.mitsgwalior.in/images/slider2/tour.jpeg"
            class="w-100"  height="650x" width="893" alt="...">
          <div class="carousel-caption">
            <h1></h1>
          </div><!--carousel-caption-->
        </div>
      </div><!--carousel-inner-->


      <button class="carousel-control-prev" type="button" data-bs-target="#aa" data-bs-slide="prev">
        <span class="carousel-control-prev-icon"></span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#aa" data-bs-slide="next">
        <span class="carousel-control-next-icon"></span>
      </button>
    </div>
    <!-- slider div end -->\

    <div class="text-center ">
      <h1 class="wri">HELLO <span ><%=n %></span>
        <br>
        REGISTER YOUR COURSE
      </h1>
      <h3>Course Registration means the official recording of a <br> student's name to study a course or courses.</h3>
    </div>
    <!--  div text-center end -->
    <div class="container mt-4">
      <div class="row">
        <div class="col-md-4 mt-4">
          <div class="card">
            <img class=" w-100 imgfi" src="https://mits-admission-portal.onrender.com/assets/images/card_image1.jpg"
              alt="">
            <div class="card-body ">
              <h1 class="text-center">B.TECH</h1>
              <p class="text-center">B.Tech is a professional undergraduate engineering degree</p>
              <% if(btech){ %>
                <a href="/display">

                  <h4 class=" text-center "><span class="btn btn-primary">Display</span> </h4>
                </a>
                <% }else{ %>
                  <a href="" data-bs-toggle="modal" data-bs-target="#btech">

                    <h4 class=" text-center "><span class="btn btn-primary">Register</span> </h4>
                  </a>
                  <% } %>
            </div>

            <!-- card body end -->
          </div>
          <!-- card end -->
        </div>
        <!-- col-md-4 end -->

        <div class="col-md-4 mt-4">
          <div class="card">
            <img class="w-100 imgfi" src="https://mits-admission-portal.onrender.com/assets/images/card_image2.jpg"
              alt="">
            <div class="card-body">
              <h1 class="text-center">BCA</h1>
              <p class="text-center">Bca is a professional undergraduate engineering degree</p>

              <% if(bca){ %>
                <a href="/display">

                  <h4 class=" text-center "><span class="btn btn-primary">Display</span> </h4>
                </a>
                <% }else{ %>
                  <a href="" data-bs-toggle="modal" data-bs-target="#bca">

                    <h4 class=" text-center "><span class="btn btn-primary">Register</span> </h4>
                  </a>
                  <% } %>

            </div>
            <!-- card body end -->
          </div>

          <!-- card end -->

        </div>
        <!-- col-md-4 end -->

        <div class="col-md-4 mt-4">
          <div class="card">
            <img class=" imgfi w-100" src="https://mits-admission-portal.onrender.com/assets/images/card_image3.jpg"
              alt="">
            <div class="card-body">
              <h1 class="text-center">MCA</h1>
              <p class="text-center">Mca is a professional undergraduate engineering degree</p>

              <% if(mca){ %>
                <a href="/display">

                  <h4 class=" text-center "><span class="btn btn-primary">Display</span> </h4>
                </a>
                <% }else{ %>
                  <a href="" data-bs-toggle="modal" data-bs-target="#mca">

                    <h4 class=" text-center "><span class="btn btn-primary">Register</span> </h4>
                  </a>
                  <% } %>

            </div>
            <!-- card body end -->
          </div>
          <!-- card end -->
        </div>
        <!-- col-md-4 end -->
      </div>

      <!-- row end -->
    </div>
    <!-- container end -->
  </div>
  <!-- mt-4 end -->



  <!--  btech Modal -->
  <div class="modal fade" id="btech" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" >
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" >Modal title</h5>
          <button type="button" class=" btn-close" data-bs-dismiss="modal" aria-label="Close">
            
          </button>
        </div>
        <div class="modal-body">
          <form action="/courseinsert" method="post">
            <div class="mb-3">
              <label for="">Name</label>
              <input type="text" name="name" class="form-control">
            </div>
            <!-- name div form end -->
            <div class="mb-3">
              <label for="">email</label>
              <input type="email" name="email" class="form-control" value="<%=email %>" readonly>
            </div>
            <!-- email div form end -->
            <div class="mb-3">
              <label for="">Phone NO</label>
              <input type="number" name="phone" class="form-control">
            </div>
            <!-- Phone NO div form end -->
            <div class="mb-3">
              <label for="">10th number</label>
              <input type="number" name="tenth" class="form-control">
            </div>
            <!-- 10th number div form end -->
            <div class="mb-3">
              <label for="">12th number</label>
              <input type="number" name="twelth" class="form-control">
            </div>
            <!-- 12th number div form end -->
            <div class="mb-3">
              <label for="">course</label>
              <input type="text" name="course" class="form-control" value="Btech" readonly required>
            </div>
            <!-- course div form end -->
            <button class="btn btn-primary" type="submit">Submit</button>
          </form>
        </div>

      </div>
    </div>
  </div>
